{
    "require": {
        "php": ">=7.2",
        "aura/di":       "~3",
        "aura/router":   "~2",
        "aura/sqlquery": "~2",
        "apereo/phpcas":                   "@stable",
        "graylog2/gelf-php":               "@stable",
        "laminas/laminas-log":             "@stable",
        "laminas/laminas-permissions-acl": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~8"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\":      "src/Domain"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "src/Test"
        }
    }
}
