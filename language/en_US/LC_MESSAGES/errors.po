msgid "invalidDate"
msgstr "You provided an invalid date."

msgid "invalidEndDate"
msgstr "The end date must come after the start date"

msgid "invalidLogin"
msgstr "The username and password combination you entered is not correct."

msgid "noAccessAllowed"
msgstr "Sorry, you're not allowed to go there."

msgid "notLoggedIn"
msgstr "You are not logged into this site anymore."

msgid "passwordCorrupted"
msgstr "Your password is corrupted.  Please contact a system administrator for help."

msgid "missingRequiredFields"
msgstr "You did not include all the required fields."

msgid "missingStatus"
msgstr "You must provide a status"

msgid "addresses/duplicateAddress"
msgstr "You are trying to save an address that is already duplicated in the system."

msgid "designations/missingName"
msgstr "You must choose a street name"

msgid "designations/missingType"
msgstr "You must choose a designation type"

msgid "ldap/unknownUser"
msgstr "That username is not in LDAP.  You cannot set LDAP authentication if the user is not in LDAP."

msgid "streets/duplicateDesignation"
msgstr "The street already uses that name"

msgid "streets/invalidAliasType"
msgstr "You cannot create an alias of type::STREET"

msgid "subunits/missingLocation"
msgstr "Missing location"

msgid "users/missingPerson_id"
msgstr "You can only create user accounts for people who are already in the system, or are in LDAP."

msgid "users/unknownUser"
msgstr "That person does not have a user account for this application"
