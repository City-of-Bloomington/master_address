msgid "signed_in_as"
msgstr "Signed in as %s"

msgid "no_results"
msgstr "No %s found"

msgid "verify_statement"
msgstr "I, %s, verify that this information is accurate at this point in time."

msgid "missingField"
msgstr "You must provide a %s"

msgid "addresses.add"
msgstr "Add a missing address"

msgid "addresses.assign"
msgstr "Assign a new address"

msgid "addresses.import"
msgstr "Bulk import addresses from a CSV file"

